# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore paperclip uploads
/public/system

# Ignore compiled assets
/public/assets

# Ignore database.yml
/config/database.yml

# Ignore bower compiled assets
vendor/assets/javascripts/bower_components/

.ruby-version
.ruby-gemset

.env

*.DS_Store
*swp
